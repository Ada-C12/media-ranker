  <% if @vote.errors.any? %>
    <% flash.each do |key, value| %>
      <div class= "alert alert-warning" class=<%= flash_class(key)%> >
      <% unless flash[key].blank? %>
       <span><%= value %></span>
        <% end %>
        <ul>
        <% @vote.errors.each do |column, message| %>
          <li>
            <%= column %>: <%= message %>
          </li>
        <% end %>
        </ul>
      </div>
    <% end %>
  <% end %>