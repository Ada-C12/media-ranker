<h2>List of Works</h2>
Albums 
<table>
  <tr>
    <th>Title</th>
    <th>Votes</th>
    <th>Created By</th>
    <th>Published</th>
     <th>Upvote</th>

  </tr>

   <% @albums.each do |work| %>
  <tr>
    <td><%= link_to work.title, work_path(work.id) %></td>
    <td><%= work.votes%></td>
    <td><%= work.creator %></td>
    <td> <%= work.publication_year%></td>
    <td> Upvote</td>

  </tr>
  <% end %>
</table>

