    <main>
      <h2>List of Works</h2>
      <section class="media-table">
        <h4>Albums</h4>
        <table class="table">
          <thead>
            <tr>
              <th width="80">Votes</th>
              <th>Title</th>
              <th>Created By</th>
              <th width="100">Published</th>
              <th width="100">Upvote</th>
            </tr>
          </thead>
          
          <tbody>
            <%@works.where(category: "album").each do |work| %>
              <tr>
                <td>votes</td>
                <td><%= work.title%></td>
                <td><%=work.creator%></td>  
                <td><%=work.publication_year%></td>
                <td>vote/upvote</td>
              </tr>          
            <%end %>
          </tbody>
        </table>
      </section>

