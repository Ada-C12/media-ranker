<h1>List of Works</h1>
<% @categories.each do |category| %>
  <table>
    <h2><%= category.capitalize %></h2>
      <tr>
        <th>Votes</th>
        <th>Title</th>
        <th>Created By</th>
        <th>Published</th>
        <th>Upvote</th>
      </tr>
    <% @works.media_sort(category).each do |work| %>
    <tr>
      <td>Number Votes Pending</td>
      <td><%= work.title %></td>
      <td><%= work.creator %></td>
      <td><%= work.publication_year %></td>
      <td>Upvote Button Pending</td>
    </tr>
    <% end %>
  </table>
<% end %>
