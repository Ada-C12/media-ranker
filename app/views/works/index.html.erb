<h1>List of Works</h1>
<h2>Albums</h2>
<table class="table table-hover">
  <thead class="thead">
    <tr>
      <th scope="col">Votes</th>
      <th scope="col">Title</th>
      <th scope="col">Created By</th>
      <th scope="col">Published</th>
      <th scope="col">Upvote</th>
    </tr>
  </thead>
  <tbody>
    <% @albums.each do |album| %>
      <tr>
        <td>votes</td>
        <td>album.title</td>
        <td>album.creator</td>
        <td>album.publication_year</td>
        <td>upvote</td>
      </tr>
    <% end %>
  </tbody>
</table>

