<h2><%= @work.title %></h2>
<p>Created by: <%= @work.creator %></p>
<p>Published: <%= @work.publication_year %></p>
<p><%= @work.description %></p>
<section>
<%= link_to "Edit", edit_work_path( @work.id ) %>
<%= link_to "Delete", work_path( @work.id ), method: :delete, data: { confirm: "Are you sure you want to delete this Media?"} %>
</section>