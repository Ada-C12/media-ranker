<%= form_with model: @work do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :category %>
  <%= f.text_field :category %>
  <%= f.label :creator %>
  <%= f.text_field :creator %>
   <%= f.label :publication_year %>
  <%= f.text_field :publication_year %>
   <%= f.label :description %>
  <%= f.text_field :description %>
 
  
  <%= f.submit submit_button_label, class: "form-button" %>

<% end %>
