<h1>Homepages#index</h1>
<p>Find me in app/views/homepages/index.html.erb</p>

<%= render partial: "nav" %>

<h2>Media Spotlight</h2>
<ul>
  <li><%= link_to @top_1.title, work_path(@top_1.id) %></li>
  <ul>
    <li> <%= @top_1.category %> </li>
    <li> <%= @top_1.creator %> </li>
    <li> <%= @top_1.publication_year %> </li>
    <li> <%= @top_1.description %> </li>
  </ul>
</ul>

<h2>Top 10 Media</h2>
<ul>
<% @top_10.each do |work| %>
  <li> <%= link_to work.title, work_path(work.id) %> </li>
  <ul>
    <li> <%= work.category %> </li>
    <li> <%= work.creator %> </li>
    <li> <%= work.publication_year %> </li>
    <li> <%= work.description %> </li>
  </ul>
<% end %>
</ul>
