<main>
  <h2>List of Users</h2>
  <table class="table">
  <thead>
    <tr>
      <th>Username</th>
      <th>Votes</th>
      <th>Joined</th>
    </tr>
  </thead>

  <tbody>
    <tr>
      <%@users.order(:created_at).each do |user|%>
        <tr>
          <td> <%=user.username%></td>
          <td><%=user.votes.length%> </td>
          <td><%=user.created_at.strftime("%b %d, %Y")%> </td>
        </tr>
      <%end%>
    </tr>
  </tbody>
  </table>
  <%link_to "Back to Media List", works_path, class:"btn btn-secondary"%>
</main>