<h1>List of Users</h1>

<table class="table responsive" >
  <thead>
    <tr>
      <th scope="col">Username</th>
      <th scope="col">Votes</th>
      <th scope="col">Joined</th>
    </tr>
  </thead>
  <tbody>
  <%@users.each do |user|%>
    <tr>
      <td><%=user.username%></td>
      <td><%= user.votes.length%></td>
      <td><%=parse_date_time(user.joined)%></td>
    </tr>
  <%end%>
  </tbody>
</table>