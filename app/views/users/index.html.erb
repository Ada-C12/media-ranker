<h2>List of Users</h2>
<table class="table">
  <thead>
    <tr>
      <th>Username</th>
      <th>Votes</th>
      <th>Joined</th>
    </tr>
  </thead>
  <tbody>
      <% User.all.each do |user| %>
      <tr>
        <td><%= link_to user.username, user_path(user) %></td>
        <td>0</td>
        <td><%= user.joined.strftime("%b %e, %Y") %></td>
      </tr>
      <% end %>
  </tbody>
</table>

<%= link_to "Back to Media List", root_path, class: "btn btn-secondary" %>
