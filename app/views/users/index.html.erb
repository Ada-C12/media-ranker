<main>
  
  <h2>Users</h2>

  <table class ="table">
    <thead>
      <th>Username</th>
      <th>Votes</th>
      <th>Joined</th>
    </thead>

      <tbody>
        <% @users.each do |user|%>
        <tr>
            <td><%= link_to user.username, user_path(user) %></td>
            <td><%= user.votes.count %>
            <td><%= readable_date(user.created_at) %>
          </tr>
      <%end%>
    </tbody>
  </table>
</main>