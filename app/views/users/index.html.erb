<h2>List of Users</h2>
<table>
  <tr>
    <th>Username</th>
    <th>Votes</th>
    <th>Joined</th>
  </tr>
<% @users.each do |user| %>
  <tr>
    <td><%= user.username %></td>
    <td><%= user.votes.count %></td>
    <td><%= user.created_at %></td>
  </tr>
  <% end %>
</table>
<%= link_to "Back to Media List", works_path, class: "btn btn-secondary" %>
