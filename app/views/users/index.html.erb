<h2>
    List of all Users 

  </h2>
  <ul>
    <% @users.each do |user| %>
      <li>
         <%= link_to user.username, user_path( user.id ) %> has voted <%= user.votes.length %> times
         <%= link_to "(Delete)", user_path( user.id ), method: :delete, data: { confirm: "Are you sure you want to delete this user?" }, class: "index-delete-link" %> 
       </li> 
     <% end%>
  </ul