<main>
<h2>List of Users</h2>
<table class="table">
  <thead>
   <tr>
    <th>Username</th>
      <th scope="col"width= 200>Votes</th>
      <th scope="col"width= 200>Joined</th>
    </tr>
  </thead>
  <tbody>

      <% @users.each do |user|%>
      <tr>  
        <td><%= link_to user.username, user_path(user.id) %></td>
        <td><%=user.votes.count%></td>
        <td><%=user.created_at.strftime("%b %d, %Y") %></td>
      </tr>
     <%end%>  
    </tbody>
  </table>

  <a class="button"> <%= link_to "Back to Media List", root_path %></a>
</main>





