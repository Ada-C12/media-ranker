<h1>List of Users</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Username</th>
      <th scope="col">Votes</th>
      <th scope="col">Joined</th>
    </tr>
  </thead>
  <tbody>
  <% @users.length.times do |i|%>
    <tr>
      <td><%= @users[i].username %></td>
      <td><%= @users[i].votes.count %></td>
      <td><%= @users[i].created_at %></td>
    </tr>
  <% end %>
  </tbody>
</table>