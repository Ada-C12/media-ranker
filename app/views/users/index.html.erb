<main>
  <h2>List of Users</h2>
    <section>
      <table class="table">
      <thead>
        <tr></tr>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @users.each do |user| %>
          <th>Username</th>
          <th>Votes</th>
          <th>Joined</th>
          <tr>
            <td class="link_to"><%= link_to user.username, user_path(user.id) %></td>
            <td><%= user.votes.length %></td>
            <td><%= user.joined_date %></td>
          </tr>
        <% end %>
        </tbody>
      </table>
      <span class="link_to"><%= link_to 'Back to top media', root_path %></span>
    </section>
</main>