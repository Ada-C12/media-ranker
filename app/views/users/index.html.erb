<h2>List of Users</h2>
<table class="table">
  <thead>
    <tr>
      <th>Username</th>
      <th>Votes</th>
      <th>Joined</th>
    </tr>
  </thead>
  <tbody>
  
      <% @users.each do |user|%>
        <tr>
          <td><%= link_to "#{user.username}", user_path(user) %></td>
          <td><%= user.votes.count %> </td>
          <td><%= readable_date(user.created_at) %></td>
        </tr>
      <% end %>
    
  </tbody>
</table>
<div class="btn btn-secondary"><%= link_to "Back to List of Users", users_path %></div>
<div class="btn btn-primary"><%= link_to "Back to Media List", root_path%></div>
