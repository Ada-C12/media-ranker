<section class="sect-class-name">
  <% phrase = like_phrase %>
  <h2> <%= total_votetype.count %> <%= phrase %><%= s_end?(total_votetype.count)%> </h2>
  
  <table class="table">
    <thead>
      <tr>
        <th>Submited</th>
        <th>Category</th>
        <th>Title</th>
        <th>Creator</th>
      </tr>
    </thead>
    <tbody>
      <% total_votetype.each do |vote|%>
        <% vote_submitted = readable_date(vote.created_at) %>
        <% vote_work_category = vote.work.category %>
        <% vote_work_creator = vote.work.creator %>
        <tr>
          <td><%= vote_submitted %></td>
          <td> <%= vote_work_category %> </td>
          <td><%= link_to vote.work.title, work_path(vote.work_id)%></td>
          <td> <%= vote_work_creator %> </td>
        </tr>
      <% end%>
    </tbody>
  </table>
</section>