<%= form_with model: @user do |f| %>
  <div class="form-group">
    <%= f.label :username, class: "form-control" %>
    <%= f.text_field :username, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.submit "Log In", login_form_path, {}, class: "btn btn-primary"%>
  </div>
<% end %>
