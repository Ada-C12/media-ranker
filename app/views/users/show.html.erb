<h1>Show.html.erb</h1>
<p>this will be a list of votes</p>
<div class="btn btn-secondary"><%= link_to "Back to List of Users", users_path %></div>
<div class="btn btn-primary"><%= link_to "Back to Media List", root_path%></div>

<table class="table">
  <thead>
    <tr>
      <th>User</th>
      <th>Date</th>
    </tr>
  </thead>
  <tbody>
<% if @user.votes.count > 0 %>
    <% @user.votes.each do |vote| %>
      <tr>
        <td><%= link_to "#{ Work.find_by(id: vote.work_id).title }", work_path(vote.work) %></td>
        <td><%= readable_date(vote.created_at) %></td>  
      </tr>
    <% end %>
  <% end %>
   </tbody>
</table>