<section>
  <h2>User Summary: user.username</h2>
  <p>Joined site user.created_at</p>
</section>
 
<section>
<table>
  <tr>
    <th>Media Title</th>
    <th>Created By</th>
    <th>Published</th>
    <th>Category</th>
    <th>Voted on</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= link_to user.username, user_path( user.id ) %></td>
    <td><%= user.votes.length %></td>
    <td><%= user.created_at %></td>
  </tr>
<% end %>
    
</table>
</section>